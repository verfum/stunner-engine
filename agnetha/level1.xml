<?xml version="1.0" ?>
<!-- blockWorldWidth: width of blocks in world units
     blockWorldHeight: height of blocks in world units
     blocksX: width of level in blocks 
     blocksY: height of level in blocks
     This implies level width is blockWorldWidth*blocksX
     height is blockWorldHeight*blocksY -->                      
<level name = "nemesis" ver = "1.0"
     blockWorldWidth = "32.0" blockWorldHeight = "32.0"
     blocksX = "10" blocksY = "8"
     heroShipID = "0" heroStartX = "160" heroStartY = "256">

    <ships>
        <ship id="0" collisionWidth="32" collisionHeight="32" collisionOffsetX="-16" collisionOffsetY="-16"
                   imageWidth="32" imageHeight="32" imageOffsetX="-16" imageOffsetY="-16" name="The Bast"
                   defaultWeapon="basic gun">
           <!-- any weapons here are weapons available at creation -->
           <weapons>
               <!-- id links to global weapon, ammo -1 means infinite bullets -->
               <weapon id="basic gun" ammo="-1" /> 
           </weapons>

           <graphics>
               <centre animSpeed="50">
                  <image file= "..\gfx\stunner\hero_norm.png" />
                  <image file= "..\gfx\stunner\hero_fwd_1.png" />
                  <image file= "..\gfx\stunner\hero_fwd_2.png" />
               </centre>
               <up animSpeed="100">
                  <image file= "..\gfx\stunner\hero_fwd_1.png" />
                  <image file= "..\gfx\stunner\hero_fwd_2.png" />
               </up>
               <left animSpeed="50">
                  <image file= "..\gfx\stunner\hero_left_norm.png" />
                  <image file= "..\gfx\stunner\hero_left_1.png" />
                  <image file= "..\gfx\stunner\hero_left_2.png" />
               </left>
               <right animSpeed="50">
                  <image file= "..\gfx\stunner\hero_right_norm.png" />
                  <image file= "..\gfx\stunner\hero_right_1.png" />
                  <image file= "..\gfx\stunner\hero_right_2.png" />
               </right>
           </graphics>
        </ship>
    </ships>

    <weapons>
       <weapon id="basic gun" defaultAmmo="-1" collisionWidth="8" collisionHeight="8" collisionOffsetX="-4" collisionOffsetY="-4"
                   imageWidth="32" imageHeight="32" imageOffsetX="-16" imageOffsetY="-16" name="Gun 1"
                   type="bullet" damage="2">
          <graphics>
             <fire animSpeed="50">
             </fire>
             <move animSpeed="50">
             </move>
             <hit animSpeed="50">
             </hit>
          </graphics>   
       </weapon>
       
    </weapons>

